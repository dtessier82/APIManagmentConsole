<Window x:Class="APIManagmentConsole.Subscriptions"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Subscriptions" MinHeight="300" MinWidth="300" Height="Auto" Width="Auto"
        DataContext="{Binding Subscriptions, Source={StaticResource Locator}}">
    <Grid>
        <TextBlock Height="Auto" Width="Auto" Text="{Binding WelcomeTitle}" Margin="246,10,10,-10" />
        <Button Content="Load Subscriptions" Margin="10,10,527,373" Command="{Binding RefreshCommand}" />
    </Grid>
</Window>
